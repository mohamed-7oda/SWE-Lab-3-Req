{
  "info": {
    "name": "Matches API",
    "_postman_id": "12345678-1234-5678-1234-567812345678",
    "description": "API for managing todos",
    "schema": "https://schema.getpostman.com/json/collection/v2.1.0/collection.json"
  },
  "item": [
    {
      "name": "Add a match",
      "request": {
        "method": "POST",
        "header": [
          {
            "key": "Content-Type",
            "value": "application/json"
          }
        ],

        "body": {
          "mode": "raw",
          "raw": "{\n  \"title\": \"Score\",\n  \"location\"}"
        },
        "url": {
          "raw": "{{baseUrl}}/add",
          "host": ["{{baseUrl}}"],
          "path": ["add"]
        }
      },
      "response": []
    },
    {
      "name": "Get Match Titles",
      "request": {
        "method": "GET",
        "header": [],
        "url": {
          "raw": "{{baseUrl}}/match",
          "host": ["{{baseUrl}}"],
          "path": ["match"]
        }
      },
      "response": []
    },
    {
      "name": "Update Matches",
      "request": {
        "method": "PUT",
        "header": [
          {
            "key": "Content-Type",
            "value": "application/json"
          }
        ],
        "body": {
          "mode": "raw",
          "raw": "{\n  \"title\": \"Updated score\",\n}"
        },
        "url": {
          "raw": "{{baseUrl}}/matches/:id",
          "host": ["{{baseUrl}}"],
          "path": ["matches", ":id"]
        }
      },
      "response": []
    },
    {
      "name": "Delete a match",
      "request": {
        "method": "DELETE",
        "header": [],
        "url": {
          "raw": "{{baseUrl}}/matches/:id",
          "host": ["{{baseUrl}}"],
          "path": ["matches", ":id"]
        }
      },
      "response": []
    },
    {
      "name": "Get Match Scores",
      "request": {
        "method": "GET",
        "header": [],
        "url": {
          "raw": "{{baseUrl}}/scores",
          "host": ["{{baseUrl}}"],
          "path": ["scores"]
        }
      },
      "response": []
    }
  ],
  "variable": [
    {
      "key": "baseUrl",
      "value": "http://localhost:3000"
    }
  ]
}
